			function cursorAnimation()
			{
			  $("span#cursor").toggle();
			}

			$(document).ready(function()
			{
			  setInterval ( "cursorAnimation()", 600 );
			 	
			  $('input[type=text], input[type=password]').each(function(){
				
				$(this).addClass('input-xlarge').attr('placeholder', $(this).prev().text().replace(': *','')); 
				$(this).prev().hide(); 				 
				});
				
				$('#edit-mollom-captcha').prev().show();
				$('#edit-mollom-captcha').prev().prev().hide();
				$('#edit-mollom-captcha').attr('placeholder', $('#edit-mollom-captcha').prev().prev().text().replace(': *','')); 
				$('legend').css('margin-bottom' , '0px');
				$('#edit-profile-country').prev().hide();
				$('.password-strength').hide();
				$('span.password-confirm').hide();
				$('#edit-profile-country').addClass('input-xlarge');
				$('#edit-pass-pass2').css('margin-top', '5px');
				$('#edit-pass-pass1').css('margin-top', '-5px');
				$('#edit-mail').css('margin-top', '-5px');
				$('#edit-profile-full-name').css('margin-top', '-20px');
				$('#edit-profile-country').css('margin-top' , '-5px');
				$('#edit-mollom-captcha-wrapper').css('margin-top' , '-5px');
				if(jQuery.browser.msie){
					$('[placeholder]').focus(function() {
					  var input = $(this);
					  if (input.val() == input.attr('placeholder')) {
						input.val('');
						input.removeClass('placeholder');
					  }
					}).blur(function() {
					  var input = $(this);
					  if (input.val() == '' || input.val() == input.attr('placeholder')) {
						input.addClass('placeholder');
						input.val(input.attr('placeholder'));
					  }
					}).blur();
				}
			});


			</script>
			
		
